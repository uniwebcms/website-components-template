{
    "name": "tutorial",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "prebuild": "yarn node ./scripts/prebuild.js",
        "start": "yarn prebuild && docusaurus start",
        "build:dev": "yarn prebuild && docusaurus build",
        "serve:dev": "docusaurus serve --port 3000",
        "clear": "rm -rf static/img/dimensions.json && docusaurus clear",
        "build:prod": "yarn prebuild && docusaurus build --out-dir ../dist/_docs",
        "build:gh": "yarn prebuild && docusaurus build --out-dir ../dist/_docs",
        "checkUpdate": "yarn node ./scripts/checkUpdate.js"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    },
    "dependencies": {
        "@docusaurus/core": "2.4.1",
        "@docusaurus/preset-classic": "2.4.1",
        "@mdx-js/react": "^1.6.22",
        "@uniwebcms/tutorial-starter": "^1.1.2",
        "chalk": "^4.1.2",
        "dotenv": "^10.0.0",
        "prism-react-renderer": "^1.3.5",
        "prop-types": "^15.8.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "2.4.1"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=16.14"
    }
}
